<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="create_system_coordinate_data_request_table" author="pveeckhout">
        <createTable tableName="system_coordinate_data_request">
            <column name="requesting_module" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            <column name="system_name" type="text">
                <constraints nullable="false" />
            </column>
        </createTable>

        <addPrimaryKey tableName="request_data_message" columnNames="topic,message" constraintName="pk_request_data_message" />
    </changeSet>

</databaseChangeLog>
